{
  "name": "agent-cli",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node --quiet src/cli.ts",
    "agent1": "node dist/cli.js agent1",
    "agent2": "node dist/cli.js agent2",
    "agent3": "node dist/cli.js agent3",
    "agent4": "node dist/cli.js agent4",
    "agent5": "node dist/cli.js agent5",
    "agent6": "node dist/cli.js agent6",
    "react": "node dist/cli.js react",
    "dev:agent1": "ts-node --transpile-only src/cli.ts agent1",
    "dev:agent2": "ts-node --transpile-only src/cli.ts agent2",
    "dev:agent3": "ts-node --transpile-only src/cli.ts agent3",
    "dev:agent4": "ts-node --transpile-only src/cli.ts agent4",
    "dev:agent5": "ts-node --transpile-only src/cli.ts agent5",
    "dev:agent6": "ts-node --transpile-only src/cli.ts agent6",
    "dev:react": "ts-node --transpile-only src/cli.ts react"
  },
  "bin": {
    "agent-cli": "./src/cli.ts"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "openai": "^4.67.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "typescript": "^5.6.3"
  }
}
